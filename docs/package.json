{
  "name": "@lage-run/docs",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "tsc && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^1.2.0",
    "@docusaurus/core": "3.8.1",
    "@docusaurus/plugin-client-redirects": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@tailwindcss/postcss": "^4.0.17",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^18.0.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.0.1",
    "react-dom": "^18.0.1",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.0.0"
  },
  "resolutions": {
    "@algolia/autocomplete-js@npm:^1.8.2": "1.17.9",
    "@cmfcmf/docusaurus-search-local@npm:1.2.0/cheerio": "1.0.0-rc.12"
  },
  "rationale": {
    "dependencies": {
      "@cmfcmf/docusaurus-search-local": "implicit peer of docusaurus?",
      "@docusaurus/*@3.8.1": "3.9.0 introduces a Node 20 requirement"
    },
    "resolutions": {
      "@algolia/autocomplete-js@npm:^1.8.2": "prevent pulling in tree that introduces versions inconsistent with @docsearch/react's old pinned deps",
      "@cmfcmf/docusaurus-search-local@npm:1.2.0/cheerio": "docusaurus depends on a specific file path that was removed after this version"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "*.md",
          "*.mdx"
        ],
        "options": {
          "printWidth": 80,
          "trailingComma": "none"
        }
      }
    ]
  },
  "packageManager": "yarn@4.12.0"
}
