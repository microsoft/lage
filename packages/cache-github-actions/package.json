{
  "name": "@lage-run/cache-github-actions",
  "version": "0.1.29",
  "description": "Github Action Cache for Lage",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/lage"
  },
  "homepage": "https://microsoft.github.io/lage/",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn types && yarn transpile",
    "transpile": "monorepo-scripts transpile",
    "types": "yarn run -T tsc",
    "lint": "monorepo-scripts lint"
  },
  "dependencies": {
    "@actions/cache": "3.3.0",
    "backfill-config": "6.7.1",
    "backfill-logger": "5.4.0",
    "workspace-tools": "0.41.0"
  },
  "devDependencies": {
    "@lage-run/monorepo-scripts": "workspace:^"
  },
  "files": [
    "lib/!(__*)",
    "lib/!(__*)/**"
  ]
}
