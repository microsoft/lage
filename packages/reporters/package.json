{
  "name": "@lage-run/reporters",
  "version": "1.3.8",
  "description": "Log reporters for Lage",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/lage"
  },
  "homepage": "https://microsoft.github.io/lage/",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn types && yarn transpile",
    "transpile": "monorepo-scripts transpile",
    "types": "yarn run -T tsc",
    "test": "yarn run -T jest",
    "lint": "monorepo-scripts lint"
  },
  "dependencies": {
    "@lage-run/format-hrtime": "workspace:^",
    "@lage-run/logger": "workspace:^",
    "@lage-run/scheduler-types": "workspace:^",
    "@lage-run/target-graph": "workspace:^",
    "@ms-cloudpack/task-reporter": "0.19.0",
    "ansi-regex": "5.0.1",
    "chalk": "4.1.2",
    "gradient-string": "2.0.2"
  },
  "devDependencies": {
    "@lage-run/monorepo-scripts": "workspace:^",
    "memory-streams": "^0.1.3"
  },
  "files": [
    "lib/!(__*)",
    "lib/!(__*)/**"
  ]
