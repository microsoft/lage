{
  "name": "@lage-run/cli",
  "version": "0.4.25",
  "description": "Command Line Interface for Lage",
  "repository": {
    "url": "https://github.com/microsoft/lage"
  },
  "bin": {
    "lage": "./bin/lage"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./lib/cli.js": {
      "types": "./lib/types/cli.d.ts",
      "import": "./lib/esm/cli.js",
      "require": "./lib/cjs/cli.js"
    }
  },
  "main": "./lib/cjs/index.cjs",
  "types": "lib/cjs/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsc -w --preserveWatchOutput",
    "test": "jest",
    "lint": "monorepo-scripts lint"
  },
  "dependencies": {
    "@lage-run/find-npm-client": "^0.1.1",
    "@lage-run/logger": "^1.2.0",
    "@lage-run/scheduler": "^0.5.2",
    "@lage-run/scheduler-types": "^0.2.2",
    "@lage-run/target-graph": "^0.5.1",
    "@lage-run/cache": "^0.1.26",
    "@lage-run/reporters": "^0.2.33",
    "commander": "^9.4.0",
    "workspace-tools": "^0.29.0",
    "chokidar": "3.5.3",
    "fast-glob": "^3.2.11",
    "cosmiconfig": "^7.0.0"
  },
  "devDependencies": {
    "@types/cosmiconfig": "6.0.0",
    "@lage-run/monorepo-fixture": "*",
    "backfill-config": "^6.3.0",
    "monorepo-scripts": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
