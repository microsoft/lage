{
  "name": "@lage-run/scheduler",
  "version": "0.5.6",
  "description": "Scheduler for Lage",
  "repository": {
    "url": "https://github.com/microsoft/lage"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./lib/workers/targetWorker.js": {
      "types": "./lib/types/workers/targetWorker.d.ts",
      "import": "./lib/esm/workers/targetWorker.js",
      "require": "./lib/cjs/workers/targetWorker.js"
    }
  },
  "main": "./lib/cjs/index.cjs",
  "types": "lib/cjs/index.d.ts",
  "scripts": {
    "build": "monorepo-scripts tsc",
    "start": "monorepo-scripts tsc -w --preserveWatchOutput",
    "test": "monorepo-scripts jest",
    "lint": "monorepo-scripts lint"
  },
  "dependencies": {
    "@lage-run/target-graph": "^0.5.3",
    "@lage-run/logger": "^1.2.2",
    "@lage-run/cache": "^0.1.28",
    "@lage-run/worker-threads-pool": "^0.4.3"
  },
  "devDependencies": {
    "@lage-run/scheduler-types": "^0.2.5",
    "monorepo-scripts": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
