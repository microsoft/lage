{
  "name": "lage",
  "version": "2.7.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/lage"
  },
  "homepage": "https://microsoft.github.io/lage/",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lage": "dist/lage.js"
  },
  "scripts": {
    "prebundle": "node scripts/prebuild.js",
    "bundle": "yarn dts-bundle && node scripts/bundle.mjs",
    "dts-bundle": "dts-bundle-generator --config ./dts-bundle.config.js && node ./scripts/update-dts-bundle.js"
  },
  "dependencies": {
    "glob-hasher": "^1.3.0"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.2"
  },
  "devDependencies": {
    "@lage-run/cli": "^0.16.0",
    "@lage-run/scheduler": "^1.1.7",
    "backfill-config": "^6.4.1",
    "dts-bundle-generator": "7.2.0",
    "workspace-tools": "^0.35.0",
    "esbuild": "^0.17.18"
  },
  "files": [
    "dist/*.d.ts",
    "dist/lage.js",
    "dist/lage.js.map",
    "dist/runners/**",
    "dist/workers/**"
  ],
  "publishConfig": {
    "access": "public"
  }
}
