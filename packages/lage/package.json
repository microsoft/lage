{
  "name": "lage",
  "version": "2.5.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/lage"
  },
  "homepage": "https://microsoft.github.io/lage/",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lage": "dist/lage.js"
  },
  "scripts": {
    "prebundle": "node scripts/prebuild.js",
    "bundle": "yarn dts-bundle && rollup --config ./rollup.config.js",
    "dts-bundle": "dts-bundle-generator --config ./dts-bundle.config.js && node ./scripts/update-dts-bundle.js"
  },
  "dependencies": {
    "glob-hasher": "1.2.1"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.2"
  },
  "devDependencies": {
    "@lage-run/cli": "^0.13.2",
    "@lage-run/scheduler": "^0.11.2",
    "rollup": "2.79.1",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-alias": "3.1.9",
    "rollup-plugin-terser": "7.0.2",
    "backfill-config": "^6.3.1",
    "dts-bundle-generator": "7.2.0",
    "workspace-tools": "^0.34.0"
  },
  "files": [
    "dist/*.d.ts",
    "dist/lage.js",
    "dist/lage.js.map",
    "dist/runners/**",
    "dist/workers/**"
  ],
  "publishConfig": {
    "access": "public"
  }
}
